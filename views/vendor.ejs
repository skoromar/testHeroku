<% include header %>

<%=id%>

<!-- Container (Pricing Section) -->
<div id="pricing" class="container">
  <div class="text-center">
    <h2>Conoce nuestas categorías de servicio</h2>
    <h4>Selecciona la de tu preferencía</h4>
  </div>
  <div class="row ">

     <% var i = 0; vendors.forEach(function(vendor) { i++; %>
    <div class="col-sm-4 col-xs-12">
      <div class="panel panel-default text-center">
        <div class="panel-heading">
          <h1><%=vendor.name%></h1>
        </div>
        <div class="panel-body">
          <img src="/public/images/vendors/steak.jpg" style="width:100%"  alt="Canoeing again">
          <p><%=vendor.description%></p>
        </div>
        <a  href="/products/<%=vendor.id%>">
        <div class="panel-footer">
          <button class="btn btn-lg">Ver comida</button>
        </div>
        </a>

      </div>      
    </div>     
      <% }); %> 
  </div>
</div>




  
<% include footer %>
</body>